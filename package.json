{
  "name": "clean-arch-builder",
  "displayName": "Clean Arch builder",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createCleanStructure",
        "title": "Create Clean Structure"
      },
      {
        "command": "extension.createCleanModelandEntity",
        "title": "Create Model and Entity"
      },
      {
        "command": "extension.createCleanUsecase",
        "title": "Create Usecase"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "true",
          "command": "extension.createCleanStructure",
          "group": "clean"
        },
        {
          "when": "true",
          "command": "extension.createCleanModelandEntity",
          "group": "clean"
        },
        {
          "when": "true",
          "command": "extension.createCleanUsecase",
          "group": "clean"
        }
      ]
    }
  }, 
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.82.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6",
    "@vscode/test-electron": "^2.3.4"
  }
}
